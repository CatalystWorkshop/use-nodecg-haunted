{
	"name": "use-nodecg",
	"version": "0.4.0",
	"description": "Custom React Hooks for NodeCG",
	"keywords": [
		"custom-hooks",
		"nodecg",
		"react",
		"react-hooks",
		"typescript"
	],
	"repository": "https://github.com/hoishin/use-nodecg",
	"license": "MIT",
	"author": "Keiichiro Amemiya <hoishinxii@gmail.com>",
	"files": [
		"cjs",
		"esm",
		"src",
		"!src/__tests__"
	],
	"main": "cjs/index.js",
	"module": "esm/index.js",
	"types": "cjs/index.d.ts",
	"scripts": {
		"prebuild": "del cjs esm",
		"build": "run-s build-*",
		"build-cjs": "tsc -p tsconfig.cjs.json",
		"build-esm": "tsc -p tsconfig.esm.json",
		"format": "prettier \"**/*.{js,ts,tsx,md,yml,yaml,json}\"",
		"format-fix": "yarn format --write",
		"format-test": "yarn format --list-different",
		"lint": "eslint \"**/*.{ts,tsx,js}\"",
		"lint-fix": "yarn lint --fix",
		"prepublishOnly": "yarn build",
		"test": "run-s format-test lint && jest",
		"version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
		"version-alpha": "npm version prerelease --preid=alpha",
		"version-beta": "npm version prerelease --preid=beta"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx,md,yml,yaml,json}": [
			"prettier --write",
			"git add"
		],
		"*.{js,ts,tsx}": [
			"eslint --fix",
			"git add"
		],
		"package.json": [
			"sort-package-json",
			"git add"
		]
	},
	"dependencies": {
		"lodash.clone": "^4.5.0"
	},
	"devDependencies": {
		"@hoishin/eslint-config-ts": "^0.0.5",
		"@hoishin/prettierrc": "^2.1.1",
		"@hoishin/tsconfig": "^1.0.0",
		"@types/jest": "^24.0.9",
		"@types/lodash.clone": "^4.5.5",
		"@types/node": "^11.10.4",
		"@types/react": "^16.8.6",
		"@types/react-dom": "^16.8.2",
		"conventional-changelog-cli": "^2.0.12",
		"del-cli": "^1.1.0",
		"eslint": "^5.15.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-react-hooks": "^1.6.0",
		"husky": "^1.3.1",
		"jest": "^24.1.0",
		"lint-staged": "^8.1.5",
		"nodecg": "^1.3.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.16.4",
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"react-testing-library": "^6.0.0",
		"sort-package-json": "^1.20.0",
		"ts-jest": "^24.0.0",
		"tslib": "^1.9.3",
		"typescript": "3.3.3333"
	},
	"peerDependencies": {
		"react": "^16.8.0",
		"tslib": "^1.9.3"
	},
	"engines": {
		"node": "^8.9.0 || ^10.13.0 || ^11.2.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
